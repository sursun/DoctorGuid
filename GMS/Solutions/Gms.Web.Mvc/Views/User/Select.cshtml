@using Gms.Domain

@{
    Layout = "~/Views/Shared/EmptyLayout.cshtml";
}

<body>

    <div id="toolbar" style="padding: 5px; height: auto">
        <div>
            工号：<input name="LoginName" class="messager-input" style="width: 80px" />
            电话：<input name="Mobile1" class="messager-input" style="width: 80px" />
            <a href="#" id="entity_search" class="easyui-linkbutton search" iconcls="icon-search">搜索</a>
        </div>
    </div>
     
    <table id="entity_search_list" url="@Url.Action("List","User")" edit="@Url.Action("Get","User")" del="@Url.Action("Delete","User")">
            
    </table>
        
    
     @*记录选择的医生，便于其他页面调用*@
    <div style="display: none;">
        <input id="user_selected_Id" />
        <input id="user_selected_Name" />
        <input id="user_selected_CodeNo" />
    </div>
        
</body>
<script type="text/javascript" src="@Url.Content("/Scripts/User/User.js")"></script>
<script type="text/javascript">
    $(document).ready(function () {
        User.Init();
    });

</script>


